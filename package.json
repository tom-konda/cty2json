{
  "name": "cty2json",
  "bin": {
    "cty2json": "bin/cli.js"
  },
  "version": "0.5.1",
  "description": "This JavaScript library converts from .cty file ( Micropolis format ) to JSON.",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all --parallel ts:build-* --serial rollup:* test",
    "postbuild": "npm run doc-gen",
    "clean": "node -e \"require('shelljs').rm('-R', ['./bin','./lib', './doc', './index.js']);\"",
    "ts:build-bin": "tsc -p ./src/ts/bin",
    "ts:build-es6": "tsc -p ./src/ts/lib",
    "rollup:node": "rollup -c",
    "rollup:legacy-browser": "rollup -c rollup.legacy.config.js",
    "doc-gen": "typedoc --name Cty2JSON --readme ./README.md --target es2015 --module es2015 --includeDeclarations --out ./doc src/ts/declaration/cty2json/index.d.ts",
    "test": "mocha --compilers ts:espower-typescript/guess src/test/**.ts"
  },
  "author": "Tom Konda",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "engine-strict": true,
  "directories": {
    "test": "src/test/"
  },
  "devDependencies": {
    "@types/commander": "^2.3.31",
    "@types/jsdom": "^2.0.29",
    "@types/mocha": "^2.2.37",
    "@types/node": "^6.0.59",
    "@types/power-assert": "^1.4.29",
    "@types/tmp": "0.0.32",
    "empower": "^1.2.1",
    "espower-typescript": "^7.0.0",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.0",
    "power-assert": "^1.4.2",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "shelljs": "^0.7.6",
    "tmp": "0.0.31",
    "tslint": "^4.3.1",
    "tslint-eslint-rules": "^3.2.3",
    "typedoc": "^0.5.5",
    "typescript": "^2.1.5"
  },
  "dependencies": {
    "commander": "^2.9.0"
  }
}