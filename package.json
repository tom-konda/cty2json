{
  "name": "cty2json",
  "bin": "bin/cli.js",
  "version": "0.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "node -e \"require('shelljs').exec('npm-run-all ts:build-es* ts:build-bin test doc-gen');\"",
    "postbuild": "npm run doc-gen",
    "clean": "node -e \"require('shelljs').rm('-R', ['temp', 'bin/**','dist/**.js', 'doc/**']);\"",
    "ts:build-bin": "node -e \"require('shelljs').exec('tsc -p ./src/ts/bin');\"",
    "ts:build-es6": "node -e \"require('shelljs').exec('tsc -p ./src/ts/lib');\"",
    "ts:build-es5": "node -e \"require('shelljs').exec('tsc -p ./src/ts/lib/tsconfigs/es5');\"",
    "doc-gen": "node -e \"require('shelljs').exec('typedoc --includeDeclarations --out ./doc src/ts/custom_typings/cty2json/cty2json.d.ts')\"",
    "test": "node -e \"require('shelljs').exec('mocha --require ./src/test/espower-typescript-loader src/test/**.ts')\""
  },
  "author": "tom-konda",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "engineStrict": true,
  "devDependencies": {
    "espower-typescript": "^4.0.0",
    "jsdom": "^7.2.2",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.5.1",
    "power-assert": "^1.2.0",
    "shelljs": "^0.6.0",
    "should": "^8.0.2",
    "tslint": "^3.8.1",
    "tslint-eslint-rules": "^1.0.1",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.10",
    "typings": "^1.3.0"
  },
  "dependencies": {
    "commander": "^2.9.0"
  }
}
