{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "lib": [
            "es2015"
        ],
        "types": [ 
            "node"
        ], 
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitUseStrict": false,
        "outDir": "../../../lib",
        "rootDir": ".",
        "emitBOM": false,
        "newLine": "LF",
        "sourceMap": false
    }
}